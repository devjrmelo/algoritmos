algoritmo "media"
var
   Numero1, Numero2: inteiro
   M: real
inicio
      Escreva ("Informe um número: ")
      Leia (numero1)
      Escreva ("Informe um número: ")
      Leia (numero2)
      M <- (numero1 + numero2)/2
      Escreval ("A média entre ", Numero1, " e ", Numero2, " é: ", M)
fimalgoritmo