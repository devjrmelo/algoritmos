algoritmo "contadorinteligente"
var
   i, f, c: Inteiro
inicio
   i <- 0

   Escreval ("  CONTAGEM INTELIGENTE  ")
   Escreval ("------------------------")

   Escreva ("INÍCIO: ")
   Leia (i)
   Escreva ("FIM: ")
   Leia (f)

   Enquanto (c < f) faca
      Se (i < f) entao
         i <- i + 1
         Escreva (c)
      Fimse
   FimEnquanto

fimalgoritmo